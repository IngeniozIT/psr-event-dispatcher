{
    "name": "ingenioz-it/event-dispatcher",
    "type": "library",
    "description": "PSR-14 Event dispatcher implementation",
    "keywords": ["PSR-14", "event", "dispatcher"],
    "homepage": "https://github.com/IngeniozIT/psr-event-dispatcher",
    "license": "MIT",
    "authors": [
        {
            "name": "IngeniozIT",
            "email": "contact@ingenioz.it"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/event-dispatcher": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8",
        "phan/phan": ">=2.4.4",
        "squizlabs/php_codesniffer": ">=3.5.2"
    },
    "provide": {
        "psr/event-dispatcher-implementation": "1.0"
    },
    "autoload": {
        "psr-4": {
            "IngeniozIT\\EventDispatcher\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IngeniozIT\\EventDispatcher\\Tests\\": "tests/"
        }
    }
}
